{
  "schema_version": 1,
  "mixin": [
    "skillcraft.mixins.json",
    "skillcraft-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "skillcraft",
    "version": "${version}",
    "name": "Skillcraft",
    "description": "Offering players a new way to progress and unlock new items, blocks, and gear. With this mod, players will have to level up specific skills to access these rewards, creating a sense of achievement and challenge.",
    "authors": ["bmjo", "BMJO"],
    "contact": {
      "sources": "https://github.com/architectury/architectury-templates"
    },
    "license": "ARR",
    "icon": "assets/skillcraft/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.bmjo.skillcraft.quilt.SkillcraftQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
      },
      {
        "id": "architectury",
        "version": ">=6.5.77"
      }
    ]
  }
}