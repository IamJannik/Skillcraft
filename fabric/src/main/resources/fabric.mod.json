{
  "schemaVersion": 1,
  "id": "skillcraft",
  "version": "${version}",
  "name": "Skillcraft",
  "description": "Offering players a new way to progress and unlock new items, blocks, and gear. With this mod, players will have to level up specific skills to access these rewards, creating a sense of achievement and challenge.",
  "authors": [
    "BMJO",
    "satisfy"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/bmjou/Skillcraft"
  },
  "license": "ARR",
  "icon": "assets/skillcraft/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.bmjo.skillcraft.fabric.SkillcraftFabric"
    ],
    "client": [
      "net.bmjo.skillcraft.fabriclike.client.SkillcraftClientFabricLike"
    ]
  },
  "mixins": [
    "skillcraft.mixins.json",
    "skillcraft-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.19.2",
    "architectury": ">=6.5.77"
  }
}